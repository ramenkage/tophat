{
    "name": "my/project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "puzzlehunt": {
            "type": "path",
            "url": "web/modules/custom/puzzlehunt"
        }
    },
    "require": {
        "php": "^5.5.9|>=7.0.8",
        "composer/installers": "^1.9.0",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/backup_migrate": "^5.0",
        "drupal/block_access": "^1.0@beta",
        "drupal/console": "^1.9.7",
        "drupal/core": "^9.0.0",
        "drupal/core-composer-scaffold": "^9.0.0",
        "drupal/core-project-message": "^9.0.0",
        "drupal/core-recommended": "^9.0.0",
        "drupal/css_editor": "^2.0",
        "drupal/devel": "^4.0",
        "drupal/draggableviews": "^2.0",
        "drupal/empty_fields": "^1.0@beta",
        "drupal/entityreference_filter": "^1.2",
        "drupal/google_api_client": "^4.2",
        "drupal/jquery_ui": "^1.4",
        "drupal/jquery_ui_datepicker": "^1.0",
        "drupal/jquery_ui_slider": "^1.1",
        "drupal/jquery_ui_touch_punch": "^1.0",
        "drupal/linked_field": "^1.3",
        "drupal/module_builder": "^3.13",
        "drupal/puzzlehunt": "@dev",
        "drupal/rules": "^3.0",
        "drupal/select2": "^1.5",
        "drupal/slack": "^1.2.0",
        "drupal/token": "^1.6",
        "drupal/tr_rulez": "1.x-dev",
        "drupal/views_bulk_edit": "^2.4.0",
        "drupal/views_bulk_operations": "^4.1",
        "drupal/views_conditional": "^1.0.0",
        "drupal/views_field_view": "^1.0.0-beta2",
        "drush/drush": "^11.0",
        "kint-php/kint": "^3.3",
        "npm-asset/select2": "^4.0",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "config": {
        "sort-packages": true,
        "discard-changes": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/console-extend-plugin": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": [],
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ]
        },
        "merge-plugin": {
            "include": [
                "web/modules/custom/*/composer.json"
            ],
            "replace": false,
            "ignore-duplicates": true
        },
        "drupal-scaffold": {
            "locations": {
              "web-root": "web/"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
